{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "dev": {
      "outputLogs": "full",
      "persistent": true,
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "test": {
      "dependsOn": ["@codecompplat/config-vitest#build"],
      "outputs": ["coverage.json"],
      "cache": false
    }
  }
}
