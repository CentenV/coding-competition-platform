{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build", "^lint"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "//#test": {
      "cache": false
    },
    "//#test:coverage": {
      "cache": false
    },
    "//#test:watch": {
      "cache": false
    }
  }
}
